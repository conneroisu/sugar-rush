	"license": "MIT",
	"devDependencies": {
		"@tsconfig/svelte": "^5.0.2",
		"@types/node": "^20.10.3",
		"@typescript-eslint/eslint-plugin": "6.13.2",
		"@typescript-eslint/parser": "6.13.2",
		"builtin-modules": "3.3.0",
		"esbuild": "0.19.9",
		"esbuild-svelte": "^0.8.0",
		"obsidian": "^1.4.11",
		"svelte": "^4.2.8",
		"svelte-preprocess": "^5.1.1",
		"tailwindcss": "^3.3.6",
		"tslib": "2.6.2",
		"typescript": "5.3.3",
		"vitest": "^1.0.4"
	},
	"dependencies": {
		"@popperjs/core": "^2.11.8"
