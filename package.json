{
	"name": "sugar-rush",
	"version": "1.0.0",
	"description": "A oil/vinegar-like plugin allowing for the modification of the file system within a buffer inside of Obsidian",
	"main": "main.js",
	"scripts": {
		"dev": "node esbuild.config.mjs",
		"build": "tsc -noEmit -skipLibCheck && node esbuild.config.mjs production",
		"version": "node version-bump.mjs && git add manifest.json versions.json"
	},
	"keywords": ['sugar', 'sugar-rush', 'obsidian', 'plugin', 'file system', 'file', 'vim', 'nvim'],
	"author": "Conner Ohnesorge",
	"license": "MIT",
	"devDependencies": {
		"@types/node": "^16.11.6",
		"@typescript-eslint/eslint-plugin": "5.29.0",
		"@typescript-eslint/parser": "5.29.0",
		"builtin-modules": "3.3.0",
		"esbuild": "0.17.3",
		"obsidian": "latest",
		"tslib": "2.4.0",
		"typescript": "4.7.4"
	}
}
